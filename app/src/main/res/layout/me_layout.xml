<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/me_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.xyy.junparking.MeActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true">
        <ImageView
            android:id="@+id/imgtabhome4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/tabhome1"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/imgtabcheck4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/tabcheck"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/imgtabmsg4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/tabmsg"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/imgtabme4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/tabme1"
            android:layout_weight="1"/>
    </LinearLayout>
</RelativeLayout>
