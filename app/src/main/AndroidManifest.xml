<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xyy.junparking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ParkActivity"
            android:label="机场停车"
            android:theme="@style/ParkActivityTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CheckActivity"
            android:label="我的订单"
            android:theme="@style/ParkActivityTheme">
            <intent-filter>
                <action android:name="com.example.xyy.junparking.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MsgActivity"
            android:label="我的消息"
            android:theme="@style/ParkActivityTheme">
            <intent-filter>
                <action android:name="com.example.xyy.junparking.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MeActivity"
            android:label="我"
            android:theme="@style/ParkActivityTheme">
            <intent-filter>
                <action android:name="com.example.xyy.junparking.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MeInfoActivity"
            android:label="信息修改"
            android:theme="@style/ParkActivityTheme">
            <intent-filter>
                <action android:name="com.example.xyy.junparking.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CouponCodeActivity"
            android:label="优惠码"
            android:theme="@style/ParkActivityTheme">
            <intent-filter>
                <action android:name="com.example.xyy.junparking.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShareActivity"
            android:label="邀请分享"
            android:theme="@style/ParkActivityTheme">
            <intent-filter>
                <action android:name="com.example.xyy.junparking.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
</manifest>